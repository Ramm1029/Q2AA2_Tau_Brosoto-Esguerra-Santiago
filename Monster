package cs4_aa_tau_brosotoesguerrasantiago;

public class Monster extends Killer {
    public Monster(String name) {
        super(name);
        this.hp = 120;
        this.sightRadius = 100;
        this.damage = 30;
        this.speed = 110;
        this.atkSpeed = 70;
        this.position = 0;
    }
}
